{
	"name": "my-app",
	"version": "0.1.0",
	"private": true,
	"dependencies": {
		"lodash": "^4.17.20",
		"react": "16.13.1",
		"react-dom": "16.13.1",
		"styled-components": "^5.1.1"
	},
	"scripts": {
		"dev": "webpack --env.development",
		"build": "webpack --env.production",
		"start": "webpack-dev-server  --config webpack.development.js --open",
		"prod": "webpack --config webpack.production.js",
		"format": "prettier --write \"**/*.+(js|md|ts|css|scss|graphql|yml|yaml|json)\"",
		"lint": "eslint --fix src/*.js",
		"precommit": "lint-staged"
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"devDependencies": {
		"@babel/core": "^7.9.0",
		"@babel/preset-env": "^7.11.0",
		"@babel/preset-react": "^7.10.4",
		"babel-loader": "^8.1.0",
		"clean-webpack-plugin": "^3.0.0",
		"css-loader": "^4.2.1",
		"dotenv-webpack": "^3.0.0",
		"eslint": "^7.7.0",
		"eslint-config-prettier": "^6.11.0",
		"eslint-plugin-prettier": "^3.1.4",
		"file-loader": "^6.0.0",
		"html-webpack-plugin": "^4.3.0",
		"husky": "^4.2.5",
		"lint-staged": "^10.2.11",
		"prettier": "^2.0.5",
		"style-loader": "^1.2.1",
		"webpack": "^5.0.0-beta.26",
		"webpack-cli": "^3.3.12",
		"webpack-dev-server": "^3.11.0",
		"webpack-merge": "^5.1.2"
	}
}
